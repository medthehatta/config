function _source_if_present {
    [ -f "$1" ] && . "$1"
}


_source_if_present "$HOME/.key-exports"
export PATH="$HOME/.local/bin:${KREW_ROOT:-$HOME/.krew}/bin:$PATH"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/ubuntu/install/google-cloud-sdk/path.bash.inc' ]; then . '/home/ubuntu/install/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/ubuntu/install/google-cloud-sdk/completion.bash.inc' ]; then . '/home/ubuntu/install/google-cloud-sdk/completion.bash.inc'; fi

